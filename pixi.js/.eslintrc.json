{
  "env": {
    "browser": true,
    "es6": true,
    "node": false
  },
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js"],
        "paths": ["./src"]
      }
    }
  }
}
